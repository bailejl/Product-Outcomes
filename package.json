  "scripts": {
    "dev": "concurrently \"npx nx serve api\" \"npx nx serve web\"",
    "build": "npx nx run-many --target=build --all",
    "test": "npx nx run-many --target=test --all --watchAll=false",
    "test:coverage": "npx nx run-many --target=test --all --coverage --watchAll=false",
    "e2e": "npx nx e2e web-e2e",
    "lint": "npx nx run-many --target=lint --all",
    "typecheck": "npx nx run-many --target=typecheck --all",
    "format": "npx prettier --write .",
    "quality": "npm run format && npm run test:coverage && npm run lint && npm audit --audit-level=high",
    "clean": "npx nx reset && rm -rf node_modules/.cache dist tmp",
    "setup": "npm install && npm run build && npx playwright install && echo \"run: sudo npx playwright install-deps\"",
    "setup:phase1": "npm install && docker-compose -f docker-compose.phase1.yml up -d && ./scripts/wait-for-services.sh && npm run build",
    "dev:phase1": "concurrently \"npx nx serve api\" \"npx nx serve web\"",
    "test:phase1": "npm run test && npm run e2e",
    "services:start:phase1": "docker-compose -f docker-compose.phase1.yml up -d",
    "services:stop:phase1": "docker-compose -f docker-compose.phase1.yml down",
    "services:logs:phase1": "docker-compose -f docker-compose.phase1.yml logs -f",
    "health-check": "./scripts/wait-for-services.sh",
    "phase1:verify": "npm run health-check && npm run test:phase1 && echo \"âœ… Phase 1 verification complete!\"",
    "monitoring:start": "./scripts/start-monitoring.sh",
    "monitoring:stop": "./scripts/stop-monitoring.sh",
    "monitoring:logs": "docker-compose -f docker-compose.monitoring.yml logs -f",
    "monitoring:status": "docker-compose -f docker-compose.monitoring.yml ps",
    "dev:with-monitoring": "npm run monitoring:start && npm run dev:phase1",
    "metrics:test": "curl -s http://localhost:3333/api/metrics | head -20",
    "security:audit": "node ./scripts/security-audit.js",
    "security:dependencies": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "performance:analyze": "npx nx build web --analyze",
    "performance:lighthouse": "npx lighthouse http://localhost:4200 --output json --output-path ./performance-report.json",
    "security:test": "npm run security:dependencies && npm run security:audit",
    "production:check": "npm run security:test && npm run performance:analyze && npm run test:coverage"
  },
