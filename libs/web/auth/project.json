{
  "name": "web-auth",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/web/auth/src",
  "projectType": "library",
  "tags": ["scope:web", "type:auth"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/web/auth",
        "main": "libs/web/auth/src/index.ts",
        "tsConfig": "libs/web/auth/tsconfig.lib.json",
        "assets": ["libs/web/auth/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/web/auth/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/web/auth/**/*.ts"]
      }
    }
  }
}